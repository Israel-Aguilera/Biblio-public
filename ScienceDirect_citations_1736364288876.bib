@article{QI2021101373,
title = {A customer-involved design concept evaluation based on multi-criteria decision-making fusing with preference and design values},
journal = {Advanced Engineering Informatics},
volume = {50},
pages = {101373},
year = {2021},
issn = {1474-0346},
