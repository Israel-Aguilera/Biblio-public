@incollection{WOOD1984165,
title = {Chapter 4 Melting Model of Pulsed Laser Processing},
editor = {R.F. Wood and C.W. White and R.T. Young},
series = {Semiconductors and Semimetals},
publisher = {Elsevier},
volume = {23},
pages = {165-250},
year = {1984},
issn = {0080-8784},
doi = {https://doi.org/10.1016/S0080-8784(08)62437-0},
url = {https://www.sciencedirect.com/science/article/pii/S0080878408624370},
author = {R.F. Wood and G.E. Jellison},
abstract = {Publisher Summary
This chapter presents a melting model of pulsed-laser processing. The theoretical foundation of the melting model is developed in the chapter, with an emphasis on the aspects important for laser-processing applications. The modeling involves finite-difference or finite-element solutions of the heat diffusion equations and explicitly evaluates both the temperature dependence of the thermal properties and the possibility of melting. The success of the melting theory of pulsed-laser annealing leaves little doubt that the conceptual foundation of the modeling is basically correcting the regime of nanosecond, and probably even picosecond, laser pulses. Nevertheless, some proponents of a plasma model of pulsed-laser annealing are reluctant to concede this. The chapter discusses the interaction of intense laser radiation with a semiconducting material and concludes with a discussion of future developments in the modeling.}
}
@incollection{RAZEGHI1990243,
title = {Chapter 6 LP-MOCVD Growth, Characterization, and Application of InP Material},
editor = {R.K. Willardson and Albert C. Beer},
series = {Semiconductors and Semimetals},
publisher = {Elsevier},
volume = {31},
pages = {243-355},
year = {1990},
issn = {0080-8784},
doi = {https://doi.org/10.1016/S0080-8784(08)62558-2},
url = {https://www.sciencedirect.com/science/article/pii/S0080878408625582},
author = {Manijeh Razeghi},
abstract = {Publisher Summary
This chapter describes the recent progress made on the growth, characterization, and application of indium phosphide (InP) and related compounds grown by low pressure metalorganic chemical vapor deposition (LP-MOCVD) growth technique. The chapter describes the energy-band structure and also introduces epitaxial growth techniques. The chapter describes the growth and characterization of InP using tetraethyl In (TEIn) and phosphine (PH3). The growth of InP using trimethyl In (TMIn) is described in the chapter. The application of InP epitaxial layers in the fabrication of Gunn diodes is detailed in the chapter. The use of silicon and gallium-gadolinium-garnet (GGG) substrates for the MOCVD growth of InP and GaAs is described in the chapter. The chapter also discusses optoelectronic applications utilizing layers grown by LPMOCVD. The InP-based semiconductor alloys defined as having an energy gap Eg > 2.0 eV are listed in the chapter along with the properties relevant to their crystal growth and some of their physical parameters.}
}
@incollection{WITKOP1983139,
