@incollection{COWLEY19871,
title = {18. Phase Transitions},
editor = {Kurt Sköld and David L. Price},
series = {Methods in Experimental Physics},
publisher = {Academic Press},
volume = {23},
pages = {1-67},
year = {1987},
booktitle = {Neutron Scattering},
issn = {0076-695X},
doi = {https://doi.org/10.1016/S0076-695X(08)60769-3},
url = {https://www.sciencedirect.com/science/article/pii/S0076695X08607693},
author = {R.A. Cowley},
abstract = {Publisher Summary
Phase transitions occur when the structure of a system changes. In magnetic materials this change may be from a high-temperature disordered phase in which the atomic magnetic moments are randomly oriented, to a low-temperature phase in which the magnetic moments are ordered in a well-defined structure. The most detailed tests of the theory of phase transitions have been performed using magnetic systems; neutron scattering techniques have also made a significant contribution to structural phase transitions. The chapter discusses the phase transitions in pure model systems. The structural phase transitions are divided into two types: the displacive transitions and the order-disorder transitions. The latter, such as the atomic ordering transition in β-brass or the ordering of the NO2 groups in the ferroelectric transition of NaN02, are similar to the magnetic transitions. They involve large motions of the atoms, which have a characteristic time that is much longer than the oscillatory time of the phonons. In contrast, the displacive transitions occur when the displacements of the atoms are small distances from their equilibrium positions in the ordered high-temperature structure. These order-order transitions are conveniently discussed by the soft-mode concept, in which the transition arises because of instability of the crystal against a particular mode of vibration. Structural phase transitions are more susceptible to the effects of a compressible lattice, impurities, and imperfections than are magnetic ones.}
}
