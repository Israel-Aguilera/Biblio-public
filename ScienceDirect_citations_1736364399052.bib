author = {D.S. Puri and M.P. Verma},
abstract = {Lundqvist's potential for ionic solids has been used to derive expressions for the third order elastic constants. It has been observed that the Cauchy relations amongst these constants are broken due to the presence of the many body term in this potential as in the case of the second order elastic constants. It has further been demonstrated that five of the six third order elastic constants can be evaluated from the values of the sixth third order elastic constants and the second order elastic constants of the solid. Application to NaCl and KCl leads to values of the five constants which agree fairly well with the corresponding measured values.}
}
@article{MENDILLO199355,
title = {Finite element simulation (FES): A computer modeling technique for studies of chemical modification of the ionosphere},
