author = {Philipp Hold and Fabian Ranz and Wilfried Sihn and Vera Hummel},
keywords = {Assembly, Complex Systems, Human-Centered Design, Planning, Analyzing, Cognitive Systems, Robotics},
abstract = {Abstract:
Due to the complexity of assembly processes, a high ratio of tasks is still performed by human workers. Short-cyclically changing work contents due to smaller lot sizes, especially in the varied series assembly, increases both the need for information support as well as the risk of rising physical and psychological stress. The use of technical and digital assistance systems can counter these challenges. Through the integration of information and communication technology as well as collaborative assembly technologies, hybrid cyber-physical assembly systems will emerge. Widely established assembly planning procedures only facilitate the design of purely manual work system. In this paper, two new planning approaches for digital and technical support systems in cyber physical assembly systems will be outlined and discussed with regard to synergies and delimitations of planning perspectives.}
}
@article{WILLOCX2021595,
title = {Combining bio-inspiration and ecodesign},
journal = {Procedia CIRP},
volume = {98},
pages = {595-600},
year = {2021},
note = {The 28th CIRP Conference on Life Cycle Engineering, March 10 – 12, 2021, Jaipur, India},
issn = {2212-8271},
doi = {https://doi.org/10.1016/j.procir.2021.01.160},
url = {https://www.sciencedirect.com/science/article/pii/S2212827121001918},
author = {M. Willocx and J.R. Duflou},
keywords = {biologically inspired design, ecodesign, design method, bio-inspiration, conceptual design},
abstract = {Prescriptive ecodesign tools aim to aid designers in creating more sustainable designs by providing them with general recommendations, relying on the designer to adapt and convert such recommendations into a creative concept. This class of ecodesign methods relies on - and is limited by - the creativity and previous knowledge of the designer. In contrast, bio-inspired design creates new concepts using analogical transfer from existing solutions found in nature. The resulting designs however do not necessarily have a lower environmental impact. This paper proposes to join ecodesign and bio-inspired design, combining the strengths of both and making them more accessible. The general recommendations resulting from ecodesign are used to steer the selection of relevant biological strategies, resulting in more tailored inspiration with the clear direction of improving the environmental impact. Starting from a single ecodesign recommendation, a number of diverse biological strategies are presented. Both the principles used by termites to passively regulate the temperature of their mound and the principles used by sharks and fish to reduce the drag across their skin can be used as an embodiment of the guideline to reduce energy consumption during use.}
}
