note = {9th International Conference of Materials Processing and Characterization, ICMPC-2019},
issn = {2214-7853},
doi = {https://doi.org/10.1016/j.matpr.2019.07.277},
url = {https://www.sciencedirect.com/science/article/pii/S2214785319324563},
author = {K. Mandal and S. Sarkar and S. Mitra and D. Bose},
abstract = {Now a day Wire Electrical Discharge Machining (WEDM) has become a dominating non-conventional machining process in the field of manufacturing industries. The main aim of the present work is to resolve the multi response optimization and find out the optimum process criteria of WEDM via TOPSIS method. Initially, the experiments have been carried out by Taguchi experimental technique. Thereafter, Entropy based TOPSIS has been utilised to convert the several response parameters into a single parameter. Finally, the best experimental result has been decided on the basis of parameters ranking followed by TOPSIS. The Analysis of Variance (ANOVA) has been done to justify of the optimal results. The numerical outcome shows that the best optimum input process parameters are pulse on time (1.1 µs), pulse off time (20 µs), flushing pressure (9 kg/cm2) and servo voltage (20 volt).}
}
@article{HALDER2025104580,
