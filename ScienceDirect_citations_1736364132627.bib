}
@article{SANSA2019587,
title = {A new approach for sustainable design scenarios selection: A case study in a tunisian company},
journal = {Journal of Cleaner Production},
volume = {232},
pages = {587-607},
year = {2019},
issn = {0959-6526},
doi = {https://doi.org/10.1016/j.jclepro.2019.05.299},
url = {https://www.sciencedirect.com/science/article/pii/S0959652619318359},
author = {Manel Sansa and Ahmed Badreddine and Taieb {Ben Romdhane}},
keywords = {Contextualized sustainable design, LCA, Choquet integral, Fuzzy analytic network process},
abstract = {In design, most of recent researches focused on criteria related only to the product. These criteria do not take into account the context of the organization which is related to the enterprise's system. In sustainable design, the choice of the best product design scenario is more complex. In fact, the number of issues is importantly high, the data is imprecise and the criteria are interdependent. This situation is more critical in an advanced stage of the design process where the decisions and choices will monitor the next steps of this process. The purpose of this paper is to improve the design approaches in a critical stage from an environmental, economic and social perspective. It deals with two main challenges, the first one is the identification of product design scenarios that are coherent with the organization's context, in terms of internal and external issues along with requirements of the stakeholders. The second challenge is the selection of the optimal option from a high number of identified scenarios. To this end, a new model with two main steps is proposed to deal with both challenges. The methodology is based on two steps. The first depends on a trilogy (i.e. Strategic, tactical, and operational). It generates operational scenarios on the basis of value chain-driven sustainable design strategies using SWOT (Strengths, Weakness, Opportunity, Threats), PESTEL (Political, Economic, Social, Technological, Environmental, Legal), 7S (Structure, Strategy, Shared Values, Style, Staff, Systems, Skills) to identify a multitude of scenarios. The second step is the choice of the optimal scenario that satisfies interdependent criteria using simplified life cycle assessment tools to compute sustainability impacts' indicators, the Choquet integral to minimize the number of scenarios, and the fuzzy analytic network process to select the optimal operational scenario. The proposed model is then implemented in a Tunisian company that manufactures lead acid batteries for motorcycles. The results proved the influence of the organization's context on the choice of the optimal sustainable design scenario. The organization was able to develop strategies and tactics on the basis of its strengths, weaknesses, threats and opportunities. The number of design alternatives was reduced using relevance checks based on influence and control criteria. Environmental, economic and social indicators were computed per life cycle phase in order to be used as inputs in the Choquet integral and the analytic network process algorithms. The proposed model guided the organization to adopt a coherent design scenario that respects the sustainable development criteria.}
}
@article{NAU2011313,
title = {Ramp-up of hybrid manufacturing technologies},
