@article{OLIVEIRA2022123,
title = {Lean and Green Product Development in SMEs: A Comparative Study between Small- and Medium-Sized Brazilian and Japanese Enterprises},
journal = {Journal of Open Innovation: Technology, Market, and Complexity},
volume = {8},
number = {3},
pages = {123},
year = {2022},
