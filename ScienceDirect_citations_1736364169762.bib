@article{RYDBERG1995101,
title = {Cleaner products in the Nordic countries based on the life cycle assessment approach: The Swedish product ecology project and the Nordic project for sustainable product development},
journal = {Journal of Cleaner Production},
volume = {3},
number = {1},
pages = {101-105},
