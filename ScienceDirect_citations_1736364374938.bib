pages = {34-51},
year = {2003},
issn = {1094-9968},
doi = {https://doi.org/10.1002/dir.10053},
url = {https://www.sciencedirect.com/science/article/pii/S1094996803701339},
author = {Curt J. Dommeyer and Barbara L. Gross},
abstract = {This study examines consumer knowledge of privacy-related laws and practices, and consumer awareness and use of strategies that may protect the consumer's privacy. Consumers were found to have very little knowledge of direct marketing practices and regulations. While consumers were fairly well informed on privacy-protection strategies, their use of these strategies was quite low. This study also investigates whether several consumer traits, namely gender, age, telephone number listing status, and desire to receive direct marketing solicitations, had any relationship to the consumer's awareness and use of privacy-protection strategies. It was found that males and young people were more likely than their counterparts to be aware of privacy-protection strategies. Those most likely to use privacy-protection strategies were young people and people who disliked receiving direct marketing solicitations.}
}
@article{LIN199331,
title = {An investigation of ductile fracturing in mild steel during upsetting},
journal = {International Journal of Machine Tools and Manufacture},
volume = {33},
number = {1},
pages = {31-50},
year = {1993},
issn = {0890-6955},
doi = {https://doi.org/10.1016/0890-6955(93)90062-Y},
url = {https://www.sciencedirect.com/science/article/pii/089069559390062Y},
author = {Z.C. Lin and S.Y. Lin},
abstract = {In this study, a combined application of strain-energy density failure theory and a coupled model of thermo-elastic-plastic large deformation for analysing ductile fracture that may occur in the upsetting process is described. Using this coupled model, the stress, strain and strain-energy density distributions can be determined. The flow stress in this model is taken as a function of strain, strain rate and temperature. Temperature predictions in the proposed model agree quite well with those of Phol's experiment (W. Pohl, Doctoral Dissertation, University of Stuttgart, F.R.G. (1972)). Using the strain-energy density criterion and the strain-energy density distributions, the site of fracture initiation is located by finding the relative maximum of the minimum strain-energy density function, (dW/dV)minmax. The site of fracture initiation, using this method, is predicted to be located near the equatorial surface region of a cylindrical specimen during cold forging. Once the strain-energy density at the fracture site reaches the critical value of the material, (dW/dV)c, damage is considered to have occurred and the simulation is terminated. The influences of various combinations of specimen geometry and platen friction conditions on the workability of the material are also discussed. The calculated results of the terminations of strain at the state of fracture in the upsetting process are consistent with the experimental results of Kuhn et al.}
}
@article{WANG2011419,
