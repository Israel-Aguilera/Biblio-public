note = {10th IFAC Symposium on Fault Detection, Supervision and Safety for Technical Processes SAFEPROCESS 2018},
issn = {2405-8963},
doi = {https://doi.org/10.1016/j.ifacol.2018.09.628},
url = {https://www.sciencedirect.com/science/article/pii/S2405896318323401},
author = {Manh-Hung Do and Damien Koenig and Didier Theilliol},
keywords = {Observers, Fault diagnosis, Fault detection, Uncertain linear systems, Semi-active suspension, Robust estimation},
abstract = {The main contribution of this paper is the design of robust H∞ proportional-integral (PI) observer for fault detection, isolation and magnitude estimation, applied for uncertain linear time-invariant (LTI) system. It is based on a combination of PI observer and H∞ norm in order to deal with system uncertainty and disturbance attenuation. This multi-objective problem is tackled by the solution of linear matrix inequality (LMI) feasibility problem, obtained from the analysis of estimation errors and the use of majoration lemma. The application to a real suspension system is then presented to highlight the performances of the proposed observer.}
}
@article{KHANBABAR2022101809,
title = {Framework construction for augmentation of resilience in critical infrastructure: Developing countries a case in point},
journal = {Technology in Society},
volume = {68},
pages = {101809},
year = {2022},
issn = {0160-791X},
doi = {https://doi.org/10.1016/j.techsoc.2021.101809},
url = {https://www.sciencedirect.com/science/article/pii/S0160791X21002840},
author = {Abdul Haseeb {Khan Babar} and Yousaf Ali},
keywords = {Critical infrastructure, Resilience strategies, Sustainable development, Policy formulation, Multi-criteria decision making},
abstract = {Infrastructure plays a vital role in the development and growth of a nation. Nevertheless, in modern times some infrastructure have become so significant that the future sustainability of the country is dependent on them. Such infrastructure is termed Critical Infrastructure (CI), a term coined in 1995 by the United States of America (USA). Thus, the USA and other developed countries have taken measures to protect and improve the resilience of their CI. However, CI is a new concept, therefore the developing countries are lacking a policy framework that could enhance the robustness of their CI. This paper aims to fill the gap by constructing a framework for the augmentation of resilience in the CI of developing countries like Pakistan. The formulation of the framework is achieved using the Fuzzy Analytic Hierarchy Process (FAHP) based Fuzzy VlseKriterijumska Optimizacija I Kompromisno Resenje (FVIKOR) technique. FAHP results in the identification of the energy sector as the most important CI whereas FVIKOR identifies data acquisition and monitoring systems as the best resilience strategy. The study recommends to the policymakers of the developing countries to follow the proposed framework of the study to devise relative guidelines for their implementation and ensure the future sustainability of their countries. The devising of a framework for the improvement of CI resilience for developing countries and the practicality of the research based on the case study of Pakistan are the principal novelties of the paper.}
}
@article{SOHLENIUS1992645,
title = {Concurrent Engineering},
journal = {CIRP Annals},
volume = {41},
number = {2},
pages = {645-655},
year = {1992},
issn = {0007-8506},
doi = {https://doi.org/10.1016/S0007-8506(07)63251-X},
url = {https://www.sciencedirect.com/science/article/pii/S000785060763251X},
author = {G. Sohlenius},
keywords = {Concurrent Engineering, Manufacturing, Design},
abstract = {Summary
The term Concurrent Engineering, also called Simultaneous Engineering, was used the first time in the US in 1989. It is primarily an expression for the ambition to increase the competitiveness by decreasing the lead-time and still improving quality and cost. The main methodology is to integrate the product development and the development of the design- and production processes. In this way Concurrent Engineering is a label for a development era of the manufacturing technology. To be successful Concurrent Engineering must be based on relevant theories, use efficient tools and be lead by dedicated management. Education and the ability for team work is essential for the success. Many companies are reporting good results from their use of Concurrent Engineering principles. More efficient software tools and manufacturing systems principles are being developed in research projects.}
}
@article{GRUJICIC2008300,
title = {Selection of the spraying technologies for over-coating of metal-stampings with thermo-plastics for use in direct-adhesion polymer metal hybrid load-bearing components},
journal = {Journal of Materials Processing Technology},
volume = {198},
number = {1},
pages = {300-312},
year = {2008},
issn = {0924-0136},
doi = {https://doi.org/10.1016/j.jmatprotec.2007.07.011},
url = {https://www.sciencedirect.com/science/article/pii/S092401360700684X},
author = {M. Grujicic and V. Sellappan and L. Mears and X. Xuan and Norbert Seyr and Marc Erdmann and Jochen Holzleitner},
keywords = {Polymer metal hybrids, Polymer metal adhesion, Automotive structural components, Polymer coating processes},
abstract = {The suitability of various polymer-powder spraying technologies for coating of metal-stampings used in polymer metal hybrid (PMH) load-bearing automotive-component applications is considered. The suitability of the spraying technologies is assessed with respect to a need for metal-stamping surface preparation/treatment, their ability to deposit the polymeric material without significant material degradation, the ability to selectively overcoat the metal-stamping, the resulting magnitude of the polymer-to-metal adhesion strength, durability of the polymer/metal bond with respect to prolonged exposure to high-temperature/high-humidity and mechanical/thermal fatigue service conditions, and compatibility with the automotive body-in-white (BIW) manufacturing process chain. The analysis revealed that while each of the spraying technologies has some limitations, the cold-gas dynamic-spray process appears to be the leading candidate technology for the indicated applications.}
}
@article{KIM1991127,
title = {Product performance as a unifying theme in concurrent design—II. Software},
journal = {Robotics and Computer-Integrated Manufacturing},
volume = {8},
number = {2},
pages = {127-134},
year = {1991},
issn = {0736-5845},
doi = {https://doi.org/10.1016/0736-5845(91)90029-R},
url = {https://www.sciencedirect.com/science/article/pii/073658459190029R},
author = {Steven H. Kim and Joon A. Ooi},
abstract = {Engineering design involves the satisfaction of customer needs, These needs, whether explicitly stated or only implicitly defined, must be translated into a set of specifications for a product. The subsequent stages of design, fabrication, and maintenance are dedicated to the fulfillment of these specifications. The task of concurrent design is to ensure that the product performs as specified, with fidelity and consistency. Ensuring product performance is the objective of quality assurance. Software packages can be developed to assist in this effort, ranging from the initial phase of requirements definition to production control and maintenance.}
}
@article{ZHANG2024102758,
title = {A smart system of Mass Personalization Product Service System (MP-PSS) driven by industrial modular configuration},
journal = {Advanced Engineering Informatics},
volume = {62},
pages = {102758},
year = {2024},
issn = {1474-0346},
doi = {https://doi.org/10.1016/j.aei.2024.102758},
url = {https://www.sciencedirect.com/science/article/pii/S1474034624004063},
author = {Xianyu Zhang and GuoJun Sheng and LuCheng Chen and XiaoPing Lu and Xinguo Ming and Siqi Qiu},
keywords = {Mass personalization, Product service system, Modular configuration, Industrial transformation, Mass customization},
abstract = {With the advancement of industrial intelligence technology, the Mass Personalization (MP) model is converging with Product Service Systems (PSS) to form a novel large-scale service model oriented towards personalized products, which is Mass Personalization Product Service Systems (MP-PSS). The MP-PSS, grounded in Personalized Product Service Systems, is primarily manifested in the areas of personalized orders, modular design, personalized product design and production, and personalized services throughout the entire product lifecycle. This paper provides a detailed study of MP-PSS driven by industrial intelligence from the perspectives of processes, frameworks, methods, and cases. A detailed analysis is conducted on configuration for MP-PSS, focusing on industrial modular configuration for MP-PSS. Finally, an enterprise in the automotive industry is selected as a case study for validation. The top-level framework and theory of MP-PSS can provide theoretical guidance for the transformation, upgrading, and model innovation of enterprises in the industrial value chain, and provide theoretical decision-making for enterprise managers. Moreover, the product modular configuration method based on comprehensive fuzzy evaluation, entropy value method, and analytical hierarchy process can improve the efficiency of customer customization of products and improve customer satisfaction.}
}
@article{DRAGOMIR2016206,
title = {Efficiency and Resilience in Product Design by Using Morphological Charts},
journal = {Energy Procedia},
volume = {85},
pages = {206-210},
year = {2016},
note = {EENVIRO-YRC 2015 - Bucharest},
issn = {1876-6102},
doi = {https://doi.org/10.1016/j.egypro.2015.12.218},
url = {https://www.sciencedirect.com/science/article/pii/S1876610215028830},
author = {Mihai Dragomir and Daniel Banyai and Diana Dragomir and Florin Popescu and Adrian Criste},
keywords = {Customer orientation, Smart furniture, Morphological charts},
abstract = {The paper presents the possibilities and results of using either brainstorming or morphological charts for the development of new products in the field of smart furniture. In order to ensure the best possible match between product characteristics and customer requirements and to shorten development time in terms of resilience, an application of morphological charts is presented. The process started with potential customer polling. The obtained data was centralized and processed as shown into morphological matrices and the results were validated within conceptual design experiments. Similarly, a pure brainstorming based incremental design solution is presented, as well as a combined approach, integrating both methods. Analyzed by several professionals in furniture design, the models resulted from the experiments show the potential of each method with respect to market targeted by a start-up furniture company.}
}
@article{KESHTKAR2017243,
title = {Application of MCDM for biologically based management scenario analysis in integrated catchment assessment and management},
journal = {Desalination and Water Treatment},
volume = {65},
pages = {243-251},
year = {2017},
issn = {1944-3986},
doi = {https://doi.org/10.5004/dwt.2017.20257},
url = {https://www.sciencedirect.com/science/article/pii/S1944398624121923},
author = {A.R. Keshtkar and Behnaz Asefjah and Yousef Erfanifard and Ali Afzali},
keywords = {Multiple criteria, Fuzzy AHP, Ecological, Physical, Socioeconomic, Integrated watershed management},
abstract = {ABSTRACT
The development and implementation of practical natural resources and catchment management policies require a comprehensive knowledge of the system processes (biological, physical, and socioeconomic), their complicated interactions, and how they react to different changes. The current research assessed the ecological, physical, and socioeconomic consequences of biologically based management scenarios targeting runoff and soil erosion problems in the Darenari catchment. The Darenari catchment with an approximate area of 554 ha is located in Fars Province, Iran. Three biological activities and 8 management scenarios were considered. Ecological consequences were studied using the weighted land cover area index. Physical effects were investigated applying the Soil Conservation Service hydrologic model. Economic and social effects were assessed applying the cost/benefit analysis as well as examining the outcomes of a social survey. The best scenarios were identified applying a multiple-criteria decision-making technique. A fuzzy analytic hierarchy process approach was applied to weight the criteria. The results indicated that multiple-criteria decision-making is a useful technique for demonstrating the catchment system as a whole, to integrate output from models and expert judgments.}
}
@article{HOSSAIN2023100319,
title = {Cyber-susiliency index: A comprehensive resiliency-sustainability-cybersecurity index for healthcare supply chain networks},
journal = {Decision Analytics Journal},
volume = {9},
pages = {100319},
year = {2023},
issn = {2772-6622},
doi = {https://doi.org/10.1016/j.dajour.2023.100319},
url = {https://www.sciencedirect.com/science/article/pii/S2772662223001595},
author = {Niamat Ullah Ibne Hossain and Sazid Rahman and Sharmine Akther Liza},
keywords = {Cyber-susiliency index, Resilience, Sustainability, Cybersecurity, Healthcare supply chain},
abstract = {Resilience, sustainability, and cyber-security are three measures in supply chains that define how robustly supply chains perform without disruption. Although these three measures can each be assessed separately and give us some idea of a supply chain’s capability, a combined measure of all three can provide a more holistic picture and comprehensive understanding of the system. To our knowledge, no attempt has been undertaken to formulate a combined measure. We utilized a data fusion technique called the Dempster Shafer (D–S) theory and developed a comprehensive assessment framework considering all three dimensions of resilience, sustainability, and cyber-security. Further, we combine these three measures into a single measure called the cyber-susiliency index. The proposed framework is applied to a healthcare supply chain and shows reasonable results. Yager’s recursive rule of combination further validates the D–S theory application. An index value is developed on a scale from 0 to 1, representing a numerical measure of the organization’s cyber-susiliency. The cyber-susiliency index for the healthcare supply case was calculated at 0.62 on a 1 to 0 scale.}
}
@article{CUI2023278,
title = {An integrated approach for selecting design parameters in smart product-service system conceptual design},
journal = {Procedia CIRP},
volume = {119},
pages = {278-283},
year = {2023},
note = {The 33rd CIRP Design Conference},
issn = {2212-8271},
doi = {https://doi.org/10.1016/j.procir.2023.01.003},
url = {https://www.sciencedirect.com/science/article/pii/S2212827123004754},
author = {Haoran Cui and Lin Gong and Yan Yan and Zhenchong Mo and Xi Xiang and Xin Liu},
keywords = {Conceptual design; Smart product-service system (SPSS); Binary integer programming (BIP); TRIZ; Heuristic algorithm},
abstract = {Nowadays, with the rapid development of cyber physical systems, IoT, and artificial intelligence, more and more attention has been paid to adapting the combination of smart, connected products and their generated services. Among these, smart product-service system (SPSS) with the unique features of smartness, connection, value co-creation, and data-driven has been widely concerned. Therefore, based on axiomatic design theory, the mapping from function domain to physical domain is a critical process in SPSS's conceptual design and the selecting of design parameters in physical domain has great impact on meeting multi-stakeholders’ benefits. Nevertheless, despite a few works on requirement conversion, it is not clear about the mapping mechanism under the target of value co-creation. Aiming to fill this gap, an integrated SPSS conceptual design method is proposed to solve the cross-domain mapping problem. It decomposes and reorganizes SPSS's function requirements with requirement development laws in TRIZ and divides the physical domain into product-related domain and service-related domain. Design parameters would be selected and evaluated by domain experts based on fuzzy comprehensive evaluation from different dimensions, which could represent the benefit of multi-stakeholders. Next, a binary integer programming (BIP) model with restrictions from different stakeholders would be constructed, which could be regarded as a multi-objective optimization problem. To solve the BIP model, the genetic algorithm is used and the optimal design parameters combination could be generated as the output of this process. Finally, a case study is carried out to demonstrate the feasibility and advantages of the integrated approach for cross-domain mapping in SPSS conceptual design.}
}
@article{YUNNA2012587,
title = {The ISM Analysis on Influence Factors of Cost Control in the Wind Power Construction Project},
journal = {Physics Procedia},
