abstract = {Increased globalized pressure, government policies on climate change quality characteristics have forced organizations to rethink their business operations. Generally, efficiency, customer satisfaction, responsibility, and profitability have been the key role for the organization. The readjustment customers approach and environment adjustment have imposed organization to device the actions that go on sustainable and the same time engage the expectations of customers. Green Lean Six Sigma (GLSS) is one of the inclusive approaches that reduce negative environmental impact, reduce waste from industries, and consistent improvability in industries. The most important benefits of GLSS are decreasing process variations, reduce defective products in the industry for eliminating waste that reduces energy consumption, and most probably reduces all negative environmental impacts. Environmental consideration is taken to eliminate waste and increase industry performance without the effect of negative environmental impacts. The purpose of this paper is to present an analysis of research on GLSS focusing on a systematic literature review and accelerate the organization to have ready for the application of sustainable GLSS practice by way of detailed understanding of realization.}
}
@article{AKA2021627,
title = {An inventory and production model with fuzzy parameters for the food sector},
journal = {Sustainable Production and Consumption},
volume = {26},
pages = {627-637},
year = {2021},
issn = {2352-5509},
doi = {https://doi.org/10.1016/j.spc.2020.12.033},
url = {https://www.sciencedirect.com/science/article/pii/S2352550920314329},
author = {Salih Aka and Gökhan Akyüz},
keywords = {Inventory and production model, food supply chains, food waste, fuzzy parameters, perishability},
abstract = {In the food sector, the importance of inventory and production decisions is increasing due to the perishable nature of food. As the results of these decisions, environmental, economic, and social problems arise such as shortages, food waste and production costs. The food production systems are affected by uncertainties caused by demand, delays, and inefficiencies. Thus, some efficient decision-making tools are required to deal with uncertain parameters in food inventory and production problems. In this study, storage and production activities of a catering company that produces daily meals are examined, and it is aimed to minimize the food waste and costs arising from these activities. We propose a linear programming (LP) model considering fuzzy parameters. In this way, the effects of the change in parameters on the total cost, workforce size, inventory and production decisions and food waste minimization are investigated. The presented model makes the decision to keep the raw material in inventory or to use it in production, considering the shelf life and age. Through the fuzzy parameter model, a scenario analysis including 112 different solutions were carried out. In this way, the total cost has been reduced 1.7% and a plan was proposed where production could be carried out with less labor. Similarly, 3% change in demand has been reflected in the fuzzy model, thereby 39% improvement was achieved in the amount of wasted food. The results show that fuzzy parameters have a critical impact on production, stock decisions and total cost at different levels. According to results, considering the uncertainty is an important factor to achieve the economic and environmental sustainability of the food sector.}
}
