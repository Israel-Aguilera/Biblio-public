pages = {1991-1998},
year = {2022},
note = {2nd International Conference on Functional Material, Manufacturing and Performances (ICFMMP-2021)},
issn = {2214-7853},
doi = {https://doi.org/10.1016/j.matpr.2021.09.338},
url = {https://www.sciencedirect.com/science/article/pii/S2214785321062052},
author = {Jag Mohan and Rajeev Rathi and Mahender Singh Kaswan and Somvir Singh Nain},
