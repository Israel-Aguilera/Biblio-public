keywords = {Solar energy, Right to light, Toronto, Solar thermal, Photovoltaic, Energy simulation, Urban environment},
abstract = {As highly dense urban areas are also the top energy consumers, the question as to how we can design our cities to be “solar ready”, i.e. suitable for successful integration of active and passive solar strategies in buildings, arises. Through a series of insolation simulations of typical urban morphologies found in the city of Toronto, this study will attempt to (i) identify if Toronto is “solar ready”, (ii) suggest strategies to overcome obstacles, and (iii) develop new urban formations that maximize insolation levels to promote the integration of solar strategies in our built environments.}
}
@article{ROMERO20191087,
title = {Methodology for multi-criteria design optimization of plastic products with a focus on highly competitive markets},
journal = {Procedia Manufacturing},
volume = {41},
pages = {1087-1094},
year = {2019},
note = {8th Manufacturing Engineering Society International Conference, MESIC 2019, 19-21 June 2019, Madrid, Spain},
issn = {2351-9789},
doi = {https://doi.org/10.1016/j.promfg.2019.10.037},
url = {https://www.sciencedirect.com/science/article/pii/S2351978919311990},
author = {V.J. Romero and A Sanchez},
keywords = {Plastic, Design optimization, QCDP+E, Sustainability, Concurrent engineering, Variational engineering},
abstract = {Plastic products are currently key in highly competitive markets because of their cost-efficiency and performance. However, plastic product markets are undergoing substantial changes due to new factors, such as sustainability and waste reduction, rising petrol and energy prices, and the arrival of new production technologies such as 3D printing. These changes make it necessary to update plastic product design to ensure competitiveness and market success. Several pure optimization strategies have been fully developed for manufacturing process optimization (such as LEAN or KAIZEN methodologies) that are recognized and useful (if not mandatory) in any competitive industry. However, there is a lack of equivalent methodologies being applied to product design optimization, even if a product design review is much more powerful to maximize returns. There are several advantages to an optimization methodology over other strategies, because of its easier implementation and compatibility with any existing organization or design strategy, because it is based on discrete optimization events, with application to the entire product lifecycle (from the early design phase to mature products), maximizing real profitability. The work here is focused on developing a plastic product design optimization methodology with several tools integrated with “variational” information flow to help users detect and evaluate improvements and to make information-based decisions, with the ability to be updated with experience and market changes. The application scope is initially limited to plastic products due to current improvement need, and ensuring deep approach, but also with development opportunities to other markets.}
}
@article{ESANGBEDO2021115151,
