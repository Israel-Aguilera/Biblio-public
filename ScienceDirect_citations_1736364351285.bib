author = {Sara Sarig and Nissim Garti and Reuven Azoury and Yohann Wax and Saul Perlberg},
abstract = {Calcium ion concentration versus time was measured in solutions containing admixture of 10 per cent tested urine of normals and stone-formers, during induced calcium oxalate precipitation. A Discriminating Index was formulated by statistical analysis of the data. It was found that stone-formers and normals differ significantly with respect to the measurements and the Discriminating Indices. An equation to evaluate the odds of stone-forming based on results of an individual test has been derived. The Discrimination Index may be recommended as a diagnostic tool.}
}
@article{BESCH197979,
