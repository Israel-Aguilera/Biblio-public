@article{GRANDI20243112,
title = {A methodology to guide companies in using Explainable AI-driven interfaces in manufacturing contexts},
journal = {Procedia Computer Science},
volume = {232},
pages = {3112-3120},
year = {2024},
note = {5th International Conference on Industry 4.0 and Smart Manufacturing (ISM 2023)},
issn = {1877-0509},
doi = {https://doi.org/10.1016/j.procs.2024.02.127},
url = {https://www.sciencedirect.com/science/article/pii/S1877050924003053},
author = {Fabio Grandi and Debora Zanatto and Andrea Capaccioli and Linda Napoletano and Sara Cavallaro and Margherita Peruzzini},
keywords = {explainable AI (XAI), artificial intelligence (AI), manufacturing, Human-Machine Interaction, user interface},
abstract = {Nowadays, the increasing integration of artificial intelligence (AI) technologies in manufacturing processes is raising the need of users to understand and interpret the decision-making processes of complex AI systems. Traditional black-box AI models often lack transparency, making it challenging for users to comprehend the reasoning behind their outputs. In contrast, Explainable Artificial Intelligence (XAI) techniques provide interpretability by revealing the internal mechanisms of AI models, making them more trustworthy and facilitating human-AI collaboration. In order to promote XAI models’ dissemination, this paper proposes a matrix-based methodology to design XAI-driven user interfaces in manufacturing contexts. It helps in mapping the users’ needs and identifying the “explainability visualization types” that best fits the end users’ requirements for the specific context of use. The proposed methodology was applied in the XMANAI European Project (https://ai4manufacturing.eu), aimed at creating a novel AI platform to support XAI-supported decision making in manufacturing plants. Results showed that the proposed methodology is able to guide companies in the correct implementation of XAI models, realizing the full potential of AI while ensuring human oversight and control.}
}
@article{KOBUSCH2024573,
title = {Conceptual framework for linking solution space and problem space of smart service systems in the packaging industry},
journal = {Procedia CIRP},
volume = {130},
pages = {573-577},
year = {2024},
note = {57th CIRP Conference on Manufacturing Systems 2024 (CMS 2024)},
issn = {2212-8271},
doi = {https://doi.org/10.1016/j.procir.2024.10.131},
url = {https://www.sciencedirect.com/science/article/pii/S2212827124012885},
author = {Maren Kobusch and Josef Gochermann and Khumbulani Mpofu},
keywords = {smart service systems, Packaging Industry, configure-to-order, design model},
abstract = {The packaging industry is rapidly evolving due to the integration of smart services and IoT technologies. This change is being driven by factors such as technological advances, the COVID-19 pandemic, sustainability issues and digitalisation. Regulatory requirements for product safety remain unchanged, but methods to meet these standards are rapidly evolving. Despite significant advances in automation and digitalisation, challenges remain in integrating smart services into the product portfolio. The study examines several companies in the packaging industry to identify the correlation between customer requirements and manufacturer solutions, with a focus on the customer. The authors describe the necessary framework conditions for the development of smarts services and their alignment with the customer’s problem space from the manufacturer’s perspective. By using a combination of Quality Function Deployment method and the Quadruple Diamond model, both areas are linked to ensure adaptation to meet changing customer requirements in the development of smart service systems, considering market dynamics and changes in digital maturity levels. In summary, understanding and effectively managing the interplay between the customer’s needs and the manufacturer’s solutions is critical in the evolving packaging industry. This study contributes to bridge the gap between theory and practice in the context of smart service systems and provides a practical process for solution sales, holistic product management and integrated smart service system portfolio development.}
}
@article{WEI20241310,
title = {Custom Product Requirement Analysis Method Revised with Uncertain Requirement Information},
journal = {Procedia CIRP},
volume = {130},
pages = {1310-1315},
year = {2024},
note = {57th CIRP Conference on Manufacturing Systems 2024 (CMS 2024)},
issn = {2212-8271},
