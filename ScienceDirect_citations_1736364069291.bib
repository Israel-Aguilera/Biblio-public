author = {Tapani Savolainen and Vesa Salminen},
keywords = {Modelling, plant projects, enterprise modelling, modelling methods, CIM-OSA, wall chart technique},
abstract = {A Rough Modelling Methodology is presented, using the European Prestandard ENV40 003 1990-04-18 “CIM-Systems Architecture-Framework for Enterprise modelling” (“CIM-OSA Cube”) as a guiding framework. The phases of the modelling project, Requirements Definition, Design Specification and Implementation Description, are derived from it. The “learning from others” dimension of CIM-OSA is applied by looking for reference solutions from other plant projects. Instead of the preliminary CIM-OSA modelling methods, the current industry practice methods and tools are used as the basis of the development. Othermodelling methods, like QFD, SA/SD etc. are considered to be linked into Rough Modelling Methodology. This research is been done in the project “Rough Modelling”, which is included in the Finnish national technology programme “Developing the Implementation of Plant Project Deliveries”(PRODEAL). The participating companies and organisations of the “Rough Modelling” project are the following: Wärtsilä Diesel Power Plants, AGA Consulting, IVO Engineering International, Federation of Finnish Metal Engineering and Electro technical Industries (FIMET), Helsinki University of Technology Institute of Industrial Automation, and Technology Development Center of Finland (TEKES).}
}
@article{CANO2021101634,
title = {A Scrum-based framework for new product development in the non-software industry},
