url = {https://www.sciencedirect.com/science/article/pii/S0009261423004268},
author = {Siti Fatimah Zaharah Mustafa and Mustafa {Mat Deris} and Teh Sabariah Binti {Abd Manan} and Salmia Beddu and Nur Liyana {Mohd Kamal} and Daud Mohamad and Saba Yavari and Sobia Qazi and Zarimah Hanafiah and Shefaa {Omar Abu Nassar} and Kai Lun Yeoh and Ishmail Sheriff and Wan Hanna Melini {Wan Mohtar} and Mohamed Hasnain Isa and Mohd Suffian Yusoff and Hamidi {Abdul Aziz}},
keywords = {Polycyclic aromatic hydrocarbons, Sungai Perak, Rough set theory, Principal component analysis, Environmental data},
abstract = {This paper presents application of rough set theory and PCA for modelling of similarity characteristics of PAHs from Perak River: Tanjung Belanja Bridge (TBB, l1), Water Treatment Plant Parit (WTPP, l2), Parit Town Discharge (PTD, l3), Water Treatment Plant Senin (WTPS, l4), and Water Treatment Plant Kepayang (WTPK, l5). The clustering involved PAHs as attributes and sampling stations as objects. The l1 and l3 were clustered in Simdegx,y≥0.8. The Simdegx,y≥0.7 was observed in all attributes (except l5) forming two sets of clusters. PCA showed that low molecular weight (LMW) PAHs were prominent with variances of 68.47% (naphthalene, Nap) and 27.63% (carbazole).}
