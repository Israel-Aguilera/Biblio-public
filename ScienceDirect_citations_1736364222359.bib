@article{BERTONI2020176,
title = {PSS cost engineering: A model-based approach for concept design},
journal = {CIRP Journal of Manufacturing Science and Technology},
volume = {29},
pages = {176-190},
year = {2020},
note = {New research advances on Product Service System along the Lifecycle},
issn = {1755-5817},
