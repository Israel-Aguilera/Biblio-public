author = {Kathy Beckingham Smith},
abstract = {A new method for analyzing the data of double-label incorporation experiments is presented. A quantity termed the fractional difference is used as the basis for assessing differences in the distribution of the two isotopes, as opposed to the usual isotope ratio. As part of the analysis, differences in distribution are evaluated in terms of the calculated system error limits. Those which fall outside these limits are selected as statistically significant and are subjected to complete quantitation.}
}
@article{VASSILIADIS200089,
title = {Towards quality-oriented data warehouse usage and evolution},
journal = {Information Systems},
volume = {25},
