pages = {89-115},
year = {2000},
note = {The 11th International Conference on Advanced Information System Engineering},
issn = {0306-4379},
doi = {https://doi.org/10.1016/S0306-4379(00)00011-9},
url = {https://www.sciencedirect.com/science/article/pii/S0306437900000119},
author = {Panos Vassiliadis and Mokrane Bouzeghoub and Christoph Quix},
keywords = {Data Warehousing, Repositories, Evolution, Data Quality},
abstract = {As a decision support information system, a data warehouse must provide high level quality of data and services. In the DWQ project (Foundations of Data Warehouse Quality), we have proposed how semantically rich meta-information of a data warehouse can be stored in a metadata repository. This static representation of the various perspectives of data warehouse components and their linkage to quality factors is complemented by an operational methodology on how to use these quality factors and achieve the quality goals of the users. This approach is an extension of the Goal-Question-Metric (GQM) approach, based on the idea that a quality goal is operationally defined over a concrete set of questions, i.e., algorithmic steps. The proposed approach covers the full lifecycle of the data warehouse, allows capturing the interrelationships between different quality factors and helps the interested user to organize them in order to fulfill specific quality goals. Furthermore, we prove how the quality management of the data warehouse can guide the process of data warehouse evolution, by tracking the interrelationships between the components of the data warehouse. Finally, we present a case study, as a proof of concept for the proposed methodology.}
}
@article{BONYANI2018180,
title = {Identifying and prioritizing foreign companies interested in participating in post-sanctions Iranian energy sector},
journal = {Energy Strategy Reviews},
volume = {21},
pages = {180-190},
year = {2018},
issn = {2211-467X},
doi = {https://doi.org/10.1016/j.esr.2018.05.008},
url = {https://www.sciencedirect.com/science/article/pii/S2211467X18300518},
author = {Abbas Bonyani and Moslem Alimohammadlou},
keywords = {Iran, Post-sanctions, Energy sector, Petrochemical industry},
abstract = {Ever since the sanctions on economic and financial cooperation with Iran have been lifted, leading contractors in the field of energy have expressed their interest in participating in Iran's energy sector. This study sought to propose a model for assessing and ranking foreign companies willing to take an active part in the Iranian petrochemical industry. To accomplish this, an integrated model composed of Best-Worst Method, ELECTRE III and PROMETHEE II was proposed. More specifically, to weight the criteria, Best-Worst Method was used, and to evaluate and rank them, ELECTRE and PROMETHEE methods were utilized. The data were gathered through questionnaires which collected the opinions of 8 experts in the field of the Iranian petrochemical industry. Results of data analysis revealed that in final evaluation, some well-known European companies in this area had higher positions compared to the others and those which cooperated with Iran during the sanctions.}
}
@article{DURAIRAJ200230,
