url = {https://www.sciencedirect.com/science/article/pii/S0166361510001752},
author = {Stefan Hüsig and Stefan Kohn},
keywords = {Closed CAI 1.0, Open CAI 2.0, Computer Aided Innovation, Open innovation},
abstract = {This article examines current developments in the field of CAI. The field of CAI has been driven by two major recent developments. One being the technological possibilities in the software field commonly referred to as “Web 2.0”. The other being a strategic paradigm shift from closed to open innovation in many companies. Both developments lead to a new form of CAI which have been analyzed in detail and named Open CAI 2.0. This paper discusses the benefits and challenges for software providers and users arising from this new evolutionary step of CAI.}
}
@article{KRISHNAIYER20181441,
title = {D3S Model for Sustainable Process Excellence},
journal = {Procedia Manufacturing},
volume = {26},
pages = {1441-1447},
year = {2018},
note = {46th SME North American Manufacturing Research Conference, NAMRC 46, Texas, USA},
issn = {2351-9789},
doi = {https://doi.org/10.1016/j.promfg.2018.07.100},
url = {https://www.sciencedirect.com/science/article/pii/S2351978918307716},
author = {Krishnan Krishnaiyer and F. Frank Chen and Brandon Burgess and Hamed Bouzary},
keywords = {Value Stream Map (VSM), Action Research (AR), Manufacturing Process Excellence, Supplier Inputs, Process, Outputs, Customer (SIPOC)},
abstract = {In business excellence models and lean methodologies, applying the right model for the right problem differentiates success vs. suitability of deployment. In this paper, we propose a novel modeling approach, D3S (Diagnose, Design, Deliver and Sustain), that incorporates the voice of the internal and external stakeholders assists in sustaining excellence for processes with varying complexity. The D3S method is adapted from Action Research (AR) to suit the implementation of manufacturing process excellence. In the model, we introduce a new process mapping modeling technique - IPOPM (Input, Process, Outcome, Process Map), that combines the traditional Supplier, Inputs, Process, Outputs and Customer (SIPOC) method with a Value Stream Map (VSM) for manufacturing process automation. We describe a practical implementation of the proposed approach to streamline high-volume and high variety operation case. One of the key advantages of the D3S model is its robustness to rationalize a process and enable a holistic end-to-end process to view of opportunities for improvements beyond geographic boundaries.}
}
@article{SMITH197616,
